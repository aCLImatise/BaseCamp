class: CommandLineTool
id: casm_layout.cwl
inputs:
- id: in_set_path_use
  doc: Set path of the AMOS bank to use
  type: File?
  inputBinding:
    prefix: -b
- id: in_output_conflict_positions
  doc: Output conflict positions and support counts to file
  type: File?
  inputBinding:
    prefix: -C
- id: in_set_acceptable_number
  doc: Set acceptable number of stdvs for mate-pairs, default 5
  type: long?
  inputBinding:
    prefix: -d
- id: in_set_maximum_alignment
  doc: Set maximum alignment gap length, default 10000
  type: long?
  inputBinding:
    prefix: -g
- id: in_set_identity_default
  doc: Set the minimum alignment identity, default 70
  type: double?
  inputBinding:
    prefix: -i
- id: in_set_identity_tolerance
  doc: Set the identity tolerance between repeats, default 2
  type: double?
  inputBinding:
    prefix: -I
- id: in_set_majority_needed
  doc: Set the majority needed to discern a conflict, default 70
  type: double?
  inputBinding:
    prefix: -m
- id: in_output_read_mappings
  doc: Output read mappings to file
  type: File?
  inputBinding:
    prefix: -M
- id: in_set_minimum_overlap
  doc: Set minimum overlap length for assembly, default 10
  type: long?
  inputBinding:
    prefix: -o
- id: in_only_place_reads
  doc: Only place reads with happy mate-pairs
  type: boolean?
  inputBinding:
    prefix: -p
- id: in_randomly_place_reads
  doc: "Randomly place repetitive reads into one of their copy\nlocations if they\
    \ cannot be placed via mate-pair info"
  type: boolean?
  inputBinding:
    prefix: -r
- id: in_set_generator_seed
  doc: "Set random generator seed to unsigned int, default\nseed is generated by the\
    \ system clock"
  type: long?
  inputBinding:
    prefix: -s
- id: in_include_singleton_contigs
  doc: Include singleton contigs in the output
  type: boolean?
  inputBinding:
    prefix: -S
- id: in_set_maximum_ignorable
  doc: Set maximum ignorable trim length, default 20
  type: long?
  inputBinding:
    prefix: -t
- id: in_output_tigr_contig
  doc: Output TIGR contig to file
  type: File?
  inputBinding:
    prefix: -T
- id: in_output_umd_contig
  doc: Output UMD contig to file
  type: File?
  inputBinding:
    prefix: -U
- id: in_set_coverage_default
  doc: Set the minimum alignment coverage, default 25
  type: double?
  inputBinding:
    prefix: -v
- id: in_set_coverage_tolerance
  doc: "Set the coverage tolerance between repeats, default 2\nPosition query sequences\
    \ on a reference based on the alignment"
  type: double?
  inputBinding:
    prefix: -V
- id: in_delta_file
  doc: ''
  type: string
  inputBinding:
    position: 0
outputs:
- id: out_stdout
  doc: Standard output stream
  type: stdout
- id: out_output_conflict_positions
  doc: Output conflict positions and support counts to file
  type: File?
  outputBinding:
    glob: $(inputs.in_output_conflict_positions)
- id: out_output_read_mappings
  doc: Output read mappings to file
  type: File?
  outputBinding:
    glob: $(inputs.in_output_read_mappings)
- id: out_output_tigr_contig
  doc: Output TIGR contig to file
  type: File?
  outputBinding:
    glob: $(inputs.in_output_tigr_contig)
- id: out_output_umd_contig
  doc: Output UMD contig to file
  type: File?
  outputBinding:
    glob: $(inputs.in_output_umd_contig)
hints: []
cwlVersion: v1.1
baseCommand:
- casm-layout
