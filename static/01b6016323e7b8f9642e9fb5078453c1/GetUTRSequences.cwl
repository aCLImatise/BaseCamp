class: CommandLineTool
id: GetUTRSequences.cwl
inputs:
- id: in_input
  doc: "Input file(s) in fasta format. All transcript\nsequences are from the longest\
    \ transcripts of protein\ncoding genes which may be generated by\nGetProteinCodingSequence.py"
  type: File?
  inputBinding:
    prefix: --input
- id: in_coordinate_file
  doc: "The file should contain the coordinate of start and\nstop codon. Generated\
    \ by RiboCode.[required]"
  type: File?
  inputBinding:
    prefix: --coordinateFile
- id: in_ot_put_prefix
  doc: "Prefix of output files.[required]\n"
  type: string?
  inputBinding:
    prefix: --otput_prefix
outputs:
- id: out_stdout
  doc: Standard output stream
  type: stdout
hints:
- class: DockerRequirement
  dockerPull: quay.io/biocontainers/ribominer:0.2.3.1--pyh3252c3a_0
cwlVersion: v1.1
baseCommand:
- GetUTRSequences
