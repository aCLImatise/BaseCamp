class: CommandLineTool
id: mloc2stockholm.pl.cwl
inputs:
- id: in_file
  doc: '[MANDATORY]     File to convert into stockholm format'
  type: boolean?
  inputBinding:
    prefix: -file
- id: in_split_input
  doc: "[OPTIONAL][yes||no]     REGULATES THE PROCESSING OF THE INPUT:   DEFAULT IS\
    \ \"no\"\nIF \"yes\": the stockholm format is generated out of two files:\na)\
    \ clustalw multiple alignment file, generated by locarna-program, and\nb) consensus\
    \ structure information file\nIF \"no\": the clustalw multiple alignment file\
    \ contains both, the multiple alignment AND the consensus\nstructure information.\
    \ This single file is sufficient to generate the stockholm format file."
  type: boolean?
  inputBinding:
    prefix: -split_input
- id: in_con_struct
  doc: '[OPTIONAL]      File containing the consensus structure information in the
    second line. Must be inputed, if split_input is "yes"'
  type: boolean?
  inputBinding:
    prefix: -con_struct
- id: in_interval_only
  doc: "[OPTIONAL][yes||no]     REGULATES THE USAGE OF A locaRNAP RESULT INTERVAL.\
    \ STANDARD IS \"NO\", IF \"YES\" IS CHOOSEN,\na corresponding rel_signal-file\
    \ has to be present for the cleavage adresses."
  type: boolean?
  inputBinding:
    prefix: -interval_only
- id: in_rel_signal
  doc: '[OPTIONAL]      Has to be provided, if interval_only is set to "yes". Contains
    the interval for the cleavage process.'
  type: boolean?
  inputBinding:
    prefix: -rel_signal
- id: in_mode
  doc: "[OPTIONAL][SE|RSE|NO]   Has to be set, if a relative signal has been provided\n\
    SE: SIMPLE EXTENTION MODEL\nRSE: RELATIVE EXTENTION/PRUNING MODEL\nNO: THERE AIN'T\
    \ NO MODIFICATIONS"
  type: boolean?
  inputBinding:
    prefix: -mode
outputs:
- id: out_stdout
  doc: Standard output stream
  type: stdout
hints: []
cwlVersion: v1.1
baseCommand:
- mloc2stockholm.pl
