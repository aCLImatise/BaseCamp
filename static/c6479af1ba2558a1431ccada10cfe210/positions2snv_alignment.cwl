class: CommandLineTool
id: positions2snv_alignment.pl.cwl
inputs:
- id: input
  doc: ':  Input file (snv_align-positions.tsv generated by snv pipeline)'
  type: boolean
  inputBinding:
    prefix: --input
- id: output
  doc: ':  Output file name'
  type: boolean
  inputBinding:
    prefix: --output
- id: format
  doc: ':  Alignment format (default phylip)'
  type: boolean
  inputBinding:
    prefix: --format
- id: keep_all
  doc: ': Keep all positions in alignment'
  type: boolean
  inputBinding:
    prefix: --keep-all
- id: reference_name
  doc: ': Use passed name instead of default for reference'
  type: boolean
  inputBinding:
    prefix: --reference-name
- id: verbose
  doc: ': Print more information'
  type: boolean
  inputBinding:
    prefix: --verbose
outputs: []
cwlVersion: v1.1
baseCommand:
- positions2snv_alignment.pl
