class: CommandLineTool
id: ../../../sparse_index.cwl
inputs:
- id: dbname
  doc: 'Name for the database folder. REQUIRED. '
  type: string
  inputBinding:
    prefix: --dbname
- id: update
  doc: 'Index the current RefSeq database. Overwrite SEQLIST. '
  type: boolean
  inputBinding:
    prefix: --update
- id: seq_list
  doc: 'Load in a tab-delimited file generated by "SPARSE.py query". Specify the absolute
    path in column "file_path" if your genomes are local. '
  type: string
  inputBinding:
    prefix: --seqlist
- id: n_thread
  doc: 'Number of threads to use. Default: 20 '
  type: string
  inputBinding:
    prefix: --n_thread
outputs: []
cwlVersion: v1.1
baseCommand:
- sparse
- index
