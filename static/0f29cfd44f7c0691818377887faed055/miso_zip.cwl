class: CommandLineTool
id: miso_zip.cwl
inputs:
- id: compress
  doc: 'Compress a directory containing MISO output. Takes as arguments: (1) the output
    filename of the compressed file, (2) a comma-separated list of directory names
    to be compressed. Example: --compress output.misozip dirname1,dirname2'
  type: string
  inputBinding:
    prefix: --compress
- id: uncompress
  doc: 'Uncompress a file generated by compress_miso. Takes as arguments: (1) the
    filename to be uncompressed, and (2) the directory to place the uncompressed representation
    into. Example: --uncompress output.misozip outputdir'
  type: string
  inputBinding:
    prefix: --uncompress
outputs: []
cwlVersion: v1.1
baseCommand:
- miso_zip
