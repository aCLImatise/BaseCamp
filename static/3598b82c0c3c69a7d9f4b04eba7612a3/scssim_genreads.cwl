class: CommandLineTool
id: scssim_genreads.cwl
inputs:
- id: input
  doc: sequence file (.fasta) generated by simuVars program
  type: string
  inputBinding:
    prefix: --input
- id: primers
  doc: the number of primers [Default:100000]
  type: long
  inputBinding:
    prefix: --primers
- id: gamma
  doc: a parameter controlling the number of primers used in each cycle [Default:1e-9]
  type: double
  inputBinding:
    prefix: --gamma
- id: model
  doc: profile inferred from real sequencing data
  type: string
  inputBinding:
    prefix: --model
- id: layout
  doc: read layout (SE for single end, PE for paired-end) [Default:PE]
  type: string
  inputBinding:
    prefix: --layout
- id: coverage
  doc: sequencing coverage [Default:5]
  type: double
  inputBinding:
    prefix: --coverage
- id: i_size
  doc: mean insert size for paired-end sequencing [Default:260]
  type: long
  inputBinding:
    prefix: --isize
- id: threads
  doc: number of threads to use [Default:1]
  type: long
  inputBinding:
    prefix: --threads
- id: output
  doc: the prefix of output file
  type: string
  inputBinding:
    prefix: --output
outputs: []
cwlVersion: v1.1
baseCommand:
- scssim
- genreads
