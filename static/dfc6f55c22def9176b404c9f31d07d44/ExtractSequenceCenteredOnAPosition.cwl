class: CommandLineTool
id: ExtractSequenceCenteredOnAPosition.cwl
inputs:
- id: in_fast_a_file
  doc: "CDS sequences with fasta format. All sequences are\nfrom the longest transcript\
    \ which may be generated by\nGetProteinCodingSequence.py"
  type: File?
  inputBinding:
    prefix: --fasta-file
- id: in_center
  doc: "The center position that we want to fix up.\ndefault=150"
  type: long?
  inputBinding:
    prefix: --center
- id: in_stretch
  doc: "The stretch length based on the center\nposition.default=3."
  type: long?
  inputBinding:
    prefix: --stretch
- id: in_output_prefix
  doc: "Prefix of output files.[required]\n"
  type: string?
  inputBinding:
    prefix: --output_prefix
outputs:
- id: out_stdout
  doc: Standard output stream
  type: stdout
hints:
- class: DockerRequirement
  dockerPull: quay.io/biocontainers/ribominer:0.2.3.1--pyh3252c3a_0
cwlVersion: v1.1
baseCommand:
- ExtractSequenceCenteredOnAPosition
