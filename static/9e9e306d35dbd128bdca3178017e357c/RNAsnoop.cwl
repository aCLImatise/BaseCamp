class: CommandLineTool
id: RNAsnoop.cwl
inputs:
- id: detailed_help
  doc: Print help, including all details and hidden  options, and exit
  type: boolean
  inputBinding:
    prefix: --detailed-help
- id: query
  doc: File containing the query sequence.
  type: string
  inputBinding:
    prefix: --query
- id: target
  doc: File containing the target sequence.
  type: string
  inputBinding:
    prefix: --target
- id: suffix
  doc: Specificy the suffix that was added by RNAup to  the accessibility files (default=`_u1_to_30.out')
  type: string
  inputBinding:
    prefix: --suffix
- id: from_rna_pl_fold
  doc: Specify the directory where accessibility  profile generated by RNAplfold are
    found
  type: string
  inputBinding:
    prefix: --from-RNAplfold
- id: alignment_mode
  doc: Specify if RNAsnoop gets alignments or single  sequences as input (default=off)
  type: boolean
  inputBinding:
    prefix: --alignment-mode
- id: fast_folding
  doc: Speedup of the target search  (default=`1')
  type: long
  inputBinding:
    prefix: --fast-folding
- id: extension_cost
  doc: Cost to add to each nucleotide in a duplex   (default=`0')
  type: long
  inputBinding:
    prefix: --extension-cost
- id: minimal_right_duplex
  doc: Minimal Right Duplex Energy (default=`-270')
  type: long
  inputBinding:
    prefix: --minimal-right-duplex
- id: minimal_loop_energy
  doc: Minimal Right Duplex Energy (default=`-280')
  type: long
  inputBinding:
    prefix: --minimal-loop-energy
- id: minimal_left_duplex
  doc: Minimal Left Duplex Energy (default=`-170')
  type: long
  inputBinding:
    prefix: --minimal-left-duplex
- id: minimal_duplex
  doc: Minimal Duplex Energy (default=`-1090')
  type: long
  inputBinding:
    prefix: --minimal-duplex
- id: duplex_distance
  doc: Distance between target 3' ends of two  consecutive duplexes (default=`1')
  type: long
  inputBinding:
    prefix: --duplex-distance
- id: minimal_stem_length
  doc: Minimal snoRNA stem length (default=`5')
  type: long
  inputBinding:
    prefix: --minimal-stem-length
- id: maximal_stem_length
  doc: Maximal snoRNA stem length (default=`120')
  type: long
  inputBinding:
    prefix: --maximal-stem-length
- id: minimal_duplex_box_length
  doc: Minimal distance between the duplex end and the  H/ACA box (default=`11')
  type: long
  inputBinding:
    prefix: --minimal-duplex-box-length
- id: maximal_duplex_box_length
  doc: Maximal distance between the duplex end and the  H/ACA box (default=`16')
  type: long
  inputBinding:
    prefix: --maximal-duplex-box-length
- id: minimal_snorna_stem_loop_length
  doc: Minimal number of nucleotides between the  beginning of stem loop and beginning
    of the snoRNA sequence (default=`1')
  type: long
  inputBinding:
    prefix: --minimal-snoRNA-stem-loop-length
- id: maximal_snorna_stem_loop_length
  doc: Maximal number of nucleotides between the  beginning of stem loop and beginning
    of the snoRNA sequence (default=`100000')
  type: long
  inputBinding:
    prefix: --maximal-snoRNA-stem-loop-length
- id: minimal_snorna_duplex_length
  doc: Minimal distance between duplex start and  snoRNA (default=`0')
  type: long
  inputBinding:
    prefix: --minimal-snoRNA-duplex-length
- id: maximal_snorna_duplex_length
  doc: Maximal distance between duplex start and  snoRNA (default=`0')
  type: long
  inputBinding:
    prefix: --maximal-snoRNA-duplex-length
- id: minimal_duplex_stem_energy
  doc: Minimal duplex stem energy (default=`-1370')
  type: long
  inputBinding:
    prefix: --minimal-duplex-stem-energy
- id: minimal_total_energy
  doc: Minimal total energy (default=`100000')
  type: long
  inputBinding:
    prefix: --minimal-total-energy
- id: maximal_stem_asymmetry
  doc: Maximal snoRNA stem asymmetry (default=`30')
  type: long
  inputBinding:
    prefix: --maximal-stem-asymmetry
- id: minimal_lower_stem_energy
  doc: Minimal lower stem energy (default=`100000')
  type: long
  inputBinding:
    prefix: --minimal-lower-stem-energy
- id: energy_threshold
  doc: Maximal energy difference between the mfe and  the desired suboptimal (default=`-1')
  type: string
  inputBinding:
    prefix: --energy-threshold
- id: produce_ps
  doc: Draw annotated 2D structures for a list of  dot-bracket structures (default=off)
  type: boolean
  inputBinding:
    prefix: --produce-ps
- id: output_directory
  doc: Set where the generated figures should be  stored (default=`./')
  type: string
  inputBinding:
    prefix: --output_directory
- id: direct_redraw
  doc: Outputs 2D interactions concurrently with the  interaction calculation for
    each suboptimal  interaction. The -I option should be  preferred. (default=off)
  type: boolean
  inputBinding:
    prefix: --direct-redraw
- id: from_rna_up
  doc: Specify the directory where accessibility  profiles generated by RNAup are
    found
  type: string
  inputBinding:
    prefix: --from-RNAup
outputs: []
cwlVersion: v1.1
baseCommand:
- RNAsnoop
