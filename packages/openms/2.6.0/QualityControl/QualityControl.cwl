class: CommandLineTool
id: QualityControl.cwl
inputs:
- id: in_in_cm
  doc: "*                         ConsensusXML input, generated by FeatureLinker.\
    \ (valid formats: 'consensusXML')"
  type: File?
  inputBinding:
    prefix: -in_cm
- id: in_in_raw
  doc: "MzML input (after InternalCalibration, if available) (valid formats: 'mzML')"
  type: string?
  inputBinding:
    prefix: -in_raw
- id: in_in_post_fdr
  doc: "FeatureXMLs after FDR filtering (valid formats: 'featureXML')"
  type: string?
  inputBinding:
    prefix: -in_postFDR
- id: in_out
  doc: "Output mzTab with QC information (valid formats: 'mzTab')"
  type: File?
  inputBinding:
    prefix: -out
- id: in_out_cm
  doc: "ConsensusXML with QC information (as metavalues) (valid formats: 'consensusXML')"
  type: File?
  inputBinding:
    prefix: -out_cm
- id: in_out_feat
  doc: "FeatureXMLs with QC information (as metavalues) (valid formats: 'featureXML')"
  type: string?
  inputBinding:
    prefix: -out_feat
- id: in_in_contaminants
  doc: "Proteins considered contaminants (valid formats: 'fasta')"
  type: File?
  inputBinding:
    prefix: -in_contaminants
- id: in_in_traf_o
  doc: "TrafoXMLs from MapAligners (valid formats: 'trafoXML')"
  type: File?
  inputBinding:
    prefix: -in_trafo
- id: in_ms_two_id_rate
  doc: :assume_all_target         Forces the metric to run even if target/decoy annotation
    is missing (accepts all pep_ids as target hits).
  type: boolean?
  inputBinding:
    prefix: -MS2_id_rate
- id: in_ini
  doc: Use the given TOPP INI file
  type: File?
  inputBinding:
    prefix: -ini
- id: in_threads
  doc: "Sets the number of threads allowed to be used by the TOPP tool (default: '1')"
  type: long?
  inputBinding:
    prefix: -threads
- id: in_write_ini
  doc: Writes the default configuration file
  type: File?
  inputBinding:
    prefix: -write_ini
- id: in_helphelp
  doc: Shows all options (including advanced)
  type: boolean?
  inputBinding:
    prefix: --helphelp
outputs:
- id: out_stdout
  doc: Standard output stream
  type: stdout
hints:
- class: DockerRequirement
  dockerPull: quay.io/biocontainers/openms:2.6.0--h4afb90d_0
cwlVersion: v1.1
baseCommand:
- QualityControl
