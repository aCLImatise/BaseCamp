version 1.0

task Anviscriptgethmmhitspergenecall {
  input {
    String? contigs_db
    String? hmm_source
    File? output_file
    String file_path
    String functions_txt
  }
  command <<<
    anvi_script_get_hmm_hits_per_gene_call \
      ~{file_path} \
      ~{functions_txt} \
      ~{if defined(contigs_db) then ("--contigs-db " +  '"' + contigs_db + '"') else ""} \
      ~{if defined(hmm_source) then ("--hmm-source " +  '"' + hmm_source + '"') else ""} \
      ~{if defined(output_file) then ("--output-file " +  '"' + output_file + '"') else ""}
  >>>
  runtime {
    docker: "quay.io/biocontainers/anvio-minimal:7--py_0"
  }
  parameter_meta {
    contigs_db: "Anvi'o contigs database generated by 'anvi-gen-\\ncontigs-database' (default: None)"
    hmm_source: "NAME\\nUse a specific HMM source. You can use '--list-hmm-\\nsources' flag to see a list of available resources.\\nThe default is 'None'."
    output_file: "File path to store results. (default: None)"
    file_path: "optional arguments:"
    functions_txt: "üç∫ More on `anvi-script-get-hmm-hits-per-gene-call`:"
  }
  output {
    File out_stdout = stdout()
    File out_output_file = "${in_output_file}"
  }
}