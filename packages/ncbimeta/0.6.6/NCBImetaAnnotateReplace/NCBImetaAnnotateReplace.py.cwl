class: CommandLineTool
id: NCBImetaAnnotateReplace.py.cwl
inputs:
- id: in_an_not_file
  doc: '[--version]'
  type: File
  inputBinding:
    prefix: --annotfile
- id: in_database
  doc: Path to the sqlite database generated by NCBImeta.
  type: File
  inputBinding:
    prefix: --database
- id: in_table
  doc: Table in NCBImeta database to modify
  type: string
  inputBinding:
    prefix: --table
outputs:
- id: out_stdout
  doc: Standard output stream
  type: stdout
cwlVersion: v1.1
baseCommand:
- NCBImetaAnnotateReplace.py
