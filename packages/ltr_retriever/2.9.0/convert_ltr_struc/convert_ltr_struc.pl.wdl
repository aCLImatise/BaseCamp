version 1.0

task ConvertLtrStrucpl {
  input {
    String log_dot_txt
  }
  command <<<
    convert_ltr_struc_pl \
      ~{log_dot_txt}
  >>>
  runtime {
    docker: "quay.io/biocontainers/ltr_retriever:2.9.0--0"
  }
  parameter_meta {
    log_dot_txt: "The log file generated by LTR_STRUC"
  }
  output {
    File out_stdout = stdout()
  }
}