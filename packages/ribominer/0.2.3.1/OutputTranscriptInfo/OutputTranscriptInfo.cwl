class: CommandLineTool
id: OutputTranscriptInfo.cwl
inputs:
- id: in_coordinate_file
  doc: "The file should contain the coordinate of start and\nstop codon of each transcript.\
    \ Generated by\nRiboCode.[required]"
  type: File?
  inputBinding:
    prefix: --coordinateFile
- id: in_gtf_file
  doc: Reference gene annotation in gtf format.[required]
  type: File?
  inputBinding:
    prefix: --gtfFile
- id: in_transcript_file
  doc: "transcript file with fasta format generated by\nRiboCode.[required]"
  type: File?
  inputBinding:
    prefix: --transcriptFile
- id: in_output_longest_trans_cr_pts
  doc: Longest transcripts information.
  type: string?
  inputBinding:
    prefix: --output-longest-transcrpts
- id: in_output_all_trans_cr_pts
  doc: "All transcripts information.\n"
  type: string?
  inputBinding:
    prefix: --output-all-transcrpts
outputs:
- id: out_stdout
  doc: Standard output stream
  type: stdout
hints:
- class: DockerRequirement
  dockerPull: quay.io/biocontainers/ribominer:0.2.3.1--pyh3252c3a_0
cwlVersion: v1.1
baseCommand:
- OutputTranscriptInfo
