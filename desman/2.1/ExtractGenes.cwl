#!/usr/bin/env cwl-runner

baseCommand:
- ExtractGenes.py
class: CommandLineTool
cwlVersion: v1.0
id: extractgenes.py
inputs:
- doc: '> scg_table.tsv'
  id: c
  inputBinding:
    prefix: -c
  type: string
- doc: GFF file generated by e.g. prodigal only needed if the contig names are not
    recoverable from the blast output file.
  id: gff_file
  inputBinding:
    prefix: --gfffile
  type: string
