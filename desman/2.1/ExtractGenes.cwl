class: CommandLineTool
id: ExtractGenes.py.cwl
inputs:
- id: c
  doc: '> scg_table.tsv'
  type: string
  inputBinding:
    prefix: -c
- id: gff_file
  doc: GFF file generated by e.g. prodigal only needed if the contig names are not
    recoverable from the blast output file.
  type: string
  inputBinding:
    prefix: --gfffile
outputs: []
cwlVersion: v1.1
baseCommand:
- ExtractGenes.py
