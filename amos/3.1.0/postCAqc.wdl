version 1.0

task PostCAqc {
  input {
    Boolean? file_d_dstmap
  }
  command <<<
    postCAqc \
      ~{true="-i" false="" file_d_dstmap}
  >>>
  parameter_meta {
    file_d_dstmap: "(.inserts file) and -d (dstmap) allow to specify a map from the library ids listed in the .frg file to database ids (cat_nos).  The inserts file is automatically generated by pullfrag and the dstmap is automatically generated by pfl.pl."
  }
}