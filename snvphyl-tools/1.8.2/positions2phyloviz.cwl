class: CommandLineTool
id: positions2phyloviz.pl.cwl
inputs:
- id: input
  doc: ':  Input file (snv_align-positions.tsv generated by snv pipeline)'
  type: boolean
  inputBinding:
    prefix: --input
- id: output_base
  doc: ':  Output file base name'
  type: boolean
  inputBinding:
    prefix: --output-base
- id: reference_name
  doc: ': Use passed name instead of default for reference'
  type: boolean
  inputBinding:
    prefix: --reference-name
- id: verbose
  doc: ': Print more information'
  type: boolean
  inputBinding:
    prefix: --verbose
outputs: []
cwlVersion: v1.1
baseCommand:
- positions2phyloviz.pl
