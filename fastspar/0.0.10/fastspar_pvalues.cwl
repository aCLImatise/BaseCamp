class: CommandLineTool
id: ../../../../home/ubuntu/BiocondaCli/fastspar_pvalues.cwl
inputs:
- id: c_slash_otu_table
  doc: OTU input table used to generated correlations
  type: File
  inputBinding:
    prefix: -c/--otu_table
- id: r_slash_correlation
  doc: Correlation table generated by FastSpar
  type: File
  inputBinding:
    prefix: -r/--correlation
- id: p_slash_prefix
  doc: Prefix output of bootstrap output files
  type: string
  inputBinding:
    prefix: -p/--prefix
- id: n_slash_permutations
  doc: Number of permutations/ bootstraps
  type: long
  inputBinding:
    prefix: -n/--permutations
- id: oslash_outfile
  doc: Output p-value matrix filename
  type: File
  inputBinding:
    prefix: -o/--outfile
- id: s_slash_pseudo
  doc: 'Calculate pseudo p-values rather than exact p-values (default: false)'
  type: boolean
  inputBinding:
    prefix: -s/--pseudo
- id: t_slash_threads
  doc: 'Number of threads (default: 1)'
  type: long
  inputBinding:
    prefix: -t/--threads
- id: _version_display
  doc: --version Display version information and exit
  type: boolean
  inputBinding:
    prefix: -v
- id: otu_table
  doc: ''
  type: File
  inputBinding:
    prefix: --otu_table
- id: correlation
  doc: ''
  type: File
  inputBinding:
    prefix: --correlation
- id: prefix
  doc: ''
  type: string
  inputBinding:
    prefix: --prefix
- id: permutations
  doc: ''
  type: long
  inputBinding:
    prefix: --permutations
- id: output
  doc: ''
  type: File
  inputBinding:
    prefix: --output
outputs: []
cwlVersion: v1.1
baseCommand:
- fastspar_pvalues
