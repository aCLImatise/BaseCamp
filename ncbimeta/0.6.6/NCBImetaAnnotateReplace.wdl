version 1.0

task NCBImetaAnnotateReplace.py {
  input {
    String? an_not_file
    String? database
    String? table
  }
  command <<<
    NCBImetaAnnotateReplace.py \
      ~{if defined(an_not_file) then ("--annotfile " +  '"' + an_not_file + '"') else ""} \
      ~{if defined(database) then ("--database " +  '"' + database + '"') else ""} \
      ~{if defined(table) then ("--table " +  '"' + table + '"') else ""}
  >>>
  parameter_meta {
    an_not_file: "[--version]"
    database: "Path to the sqlite database generated by NCBImeta."
    table: "Table in NCBImeta database to modify"
  }
}