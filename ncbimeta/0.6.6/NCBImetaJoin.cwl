class: CommandLineTool
id: ../../../NCBImetaJoin.py.cwl
inputs:
- id: database
  doc: Path to the sqlite database generated by NCBImeta.
  type: string
  inputBinding:
    prefix: --database
- id: anchor
  doc: Table in NCBImeta database to use as an anchor for joining
  type: string
  inputBinding:
    prefix: --anchor
- id: accessory
  doc: Accessory tables to join to the anchor as space separated str list ex. 'BioProject
    Assembly'
  type: string
  inputBinding:
    prefix: --accessory
- id: final
  doc: Name of the final table after the join
  type: string
  inputBinding:
    prefix: --final
- id: unique
  doc: Column names in anchor table, to search for values in accessory tables ex.
    'Accession BioProject'
  type: string
  inputBinding:
    prefix: --unique
outputs: []
cwlVersion: v1.1
baseCommand:
- NCBImetaJoin.py
