class: CommandLineTool
id: ../../../NCBImetaExport.py.cwl
inputs:
- id: in_database
  doc: Path to the sqlite database generated by NCBImeta.
  type: File
  inputBinding:
    prefix: --database
- id: in_output_dir
  doc: "Output directory.\n"
  type: Directory
  inputBinding:
    prefix: --outputdir
outputs:
- id: out_stdout
  doc: Standard output stream
  type: stdout
- id: out_output_dir
  doc: "Output directory.\n"
  type: Directory
  outputBinding:
    glob: $(inputs.in_output_dir)
cwlVersion: v1.1
baseCommand:
- NCBImetaExport.py
