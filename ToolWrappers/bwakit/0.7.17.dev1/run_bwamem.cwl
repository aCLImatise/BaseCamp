class: CommandLineTool
id: run_bwamem.cwl
inputs:
- id: in_prefix_output_files
  doc: prefix for output files                       [inferred from input]
  type: string?
  inputBinding:
    prefix: -o
- id: in_read_group_line
  doc: read group header line such as '@RG\tID:foo\tSM:bar'         [null]
  type: string?
  inputBinding:
    prefix: -R
- id: in_read_type_pacbio
  doc: "read type: pacbio, ont2d or intractg                      [default]\nintractg:\
    \ intra-species contig (kb query, highly similar)\npacbio:   pacbio subreads (~10kb\
    \ query, high error rate)\nont2d:    Oxford Nanopore reads (~10kb query, higher\
    \ error rate)"
  type: long?
  inputBinding:
    prefix: -x
- id: in_number_of_threads
  doc: number of threads                                               [1]
  type: long?
  inputBinding:
    prefix: -t
- id: in_apply_hla_typing
  doc: apply HLA typing
  type: boolean?
  inputBinding:
    prefix: -H
- id: in_trim_pe_adapters
  doc: trim HiSeq2000/2500 PE resequencing adapters (via trimadap)
  type: boolean?
  inputBinding:
    prefix: -a
- id: in_mark_duplicate_samblaster
  doc: mark duplicate (via samblaster)
  type: boolean?
  inputBinding:
    prefix: -d
- id: in_bam_input_shuffle
  doc: for BAM input, don't shuffle
  type: boolean?
  inputBinding:
    prefix: -S
- id: in_sort_output_alignment
  doc: sort the output alignment (via samtools; requring more RAM)
  type: boolean?
  inputBinding:
    prefix: -s
- id: in_keep_temporary_files
  doc: keep temporary files generated by typeHLA
  type: boolean?
  inputBinding:
    prefix: -k
- id: in_mark_shorter_hits
  doc: mark shorter split hits as secondary
  type: boolean?
  inputBinding:
    prefix: -M
- id: in_more_options
  doc: ''
  type: boolean?
  inputBinding:
    prefix: -MORE_OPTIONS
- id: in_options
  doc: ''
  type: boolean?
  inputBinding:
    prefix: -OPTIONS
- id: in_idx_base
  doc: ''
  type: string
  inputBinding:
    position: 0
- id: in_file_one
  doc: ''
  type: long
  inputBinding:
    position: 1
- id: in_file_two
  doc: ''
  type: long?
  inputBinding:
    position: 2
outputs:
- id: out_stdout
  doc: Standard output stream
  type: stdout
hints: []
cwlVersion: v1.1
baseCommand:
- run-bwamem
