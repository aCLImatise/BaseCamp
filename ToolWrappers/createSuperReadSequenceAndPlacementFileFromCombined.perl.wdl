version 1.0

task CreateSuperReadSequenceAndPlacementFileFromCombinedperl {
  input {
    File? mean_and_stdev_file
  }
  command <<<
    createSuperReadSequenceAndPlacementFileFromCombined_perl \
      ~{if defined(mean_and_stdev_file) then ("--mean-and-stdev-file " +  '"' + mean_and_stdev_file + '"') else ""}
  >>>
  parameter_meta {
    mean_and_stdev_file: "; file generated by\\ngetMeanAndStdevForGapsByGapNumUsingCeleraTerminatorDirectory.perl\\n"
  }
  output {
    File out_stdout = stdout()
  }
}