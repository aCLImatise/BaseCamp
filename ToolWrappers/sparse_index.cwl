class: CommandLineTool
id: sparse_index.cwl
inputs:
- id: in_dbname
  doc: Name for the database folder. REQUIRED.
  type: Directory
  inputBinding:
    prefix: --dbname
- id: in_update
  doc: Index the current RefSeq database. Overwrite SEQLIST.
  type: boolean
  inputBinding:
    prefix: --update
- id: in_seq_list
  doc: "Load in a tab-delimited file generated by \"SPARSE.py query\".\nSpecify the\
    \ absolute path in column \"file_path\" if your genomes are local."
  type: File
  inputBinding:
    prefix: --seqlist
- id: in_n_thread
  doc: "Number of threads to use. Default: 20\n"
  type: long
  inputBinding:
    prefix: --n_thread
outputs:
- id: out_stdout
  doc: Standard output stream
  type: stdout
cwlVersion: v1.1
baseCommand:
- sparse
- index
