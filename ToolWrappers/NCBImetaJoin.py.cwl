class: CommandLineTool
id: NCBImetaJoin.py.cwl
inputs:
- id: in_database
  doc: Path to the sqlite database generated by NCBImeta.
  type: File
  inputBinding:
    prefix: --database
- id: in_anchor
  doc: Table in NCBImeta database to use as an anchor for
  type: string
  inputBinding:
    prefix: --anchor
- id: in_final
  doc: Name of the final table after the join
  type: string
  inputBinding:
    prefix: --final
- id: in_unique
  doc: "Column names in anchor table, to search for values in\naccessory tables ex.\
    \ 'Accession BioProject'\n"
  type: string
  inputBinding:
    prefix: --unique
- id: in_accessory
  doc: ''
  type: boolean
  inputBinding:
    prefix: --accessory
- id: in_joining
  doc: --accessory DBACCESSORY
  type: string
  inputBinding:
    position: 0
outputs:
- id: out_stdout
  doc: Standard output stream
  type: stdout
cwlVersion: v1.1
baseCommand:
- NCBImetaJoin.py
