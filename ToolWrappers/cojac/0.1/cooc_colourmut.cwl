class: CommandLineTool
id: cooc_colourmut.cwl
inputs:
- id: in_json
  doc: results generated by mutbamscan
  type: string?
  inputBinding:
    prefix: --json
- id: in_yaml
  doc: results generated by mutbamscan
  type: string?
  inputBinding:
    prefix: --yaml
outputs:
- id: out_stdout
  doc: Standard output stream
  type: stdout
hints:
- class: DockerRequirement
  dockerPull: quay.io/biocontainers/cojac:0.1--0
cwlVersion: v1.1
baseCommand:
- cooc-colourmut
