version 1.0

task Dsk2ascii {
  input {
    Boolean? arg_output_stdout
    Boolean? fast_a
    Boolean? fast_q
    Boolean? arg_file_generated
    File? out
    Boolean? nb_cores
    Boolean? verbose
    Boolean? version
  }
  command <<<
    dsk2ascii \
      ~{if (arg_output_stdout) then "-c" else ""} \
      ~{if (fast_a) then "-fasta" else ""} \
      ~{if (fast_q) then "-fastq" else ""} \
      ~{if (arg_file_generated) then "-file" else ""} \
      ~{if (out) then "-out" else ""} \
      ~{if (nb_cores) then "-nb-cores" else ""} \
      ~{if (verbose) then "-verbose" else ""} \
      ~{if (version) then "-version" else ""}
  >>>
  runtime {
    docker: "None"
  }
  parameter_meta {
    arg_output_stdout: "(0 arg) :    output to stdout"
    fast_a: "(0 arg) :    output in fasta format, one kmer per read"
    fast_q: "(0 arg) :    output in fastq format, one kmer per read"
    arg_file_generated: "(1 arg) :    file generated by dsk"
    out: "(1 arg) :    output file"
    nb_cores: "(1 arg) :    number of cores  [default '0']"
    verbose: "(1 arg) :    verbosity level  [default '1']"
    version: "(0 arg) :    version"
  }
  output {
    File out_stdout = stdout()
    File out_out = "${in_out}"
  }
}