class: CommandLineTool
id: svtools_lmerge.cwl
inputs:
- id: in_in_file
  doc: "a sorted VCF file generated by svtools lsort. Each\nINFO field must contain\
    \ an SNAME tag containing the\nsample name (e.g. SNAME=SAMPLE_NAME)"
  type: File
  inputBinding:
    prefix: --inFile
- id: in_percent_slop
  doc: "increase the the breakpoint confidence interval both\nup and down stream by\
    \ a given proportion of the\noriginal size"
  type: double
  inputBinding:
    prefix: --percent-slop
- id: in_fixed_slop
  doc: "increase the the breakpoint confidence interval both\nup and down stream by\
    \ a given fixed size"
  type: long
  inputBinding:
    prefix: --fixed-slop
- id: in_sum
  doc: "calculate breakpoint PDF and position using sum\nalgorithm instead of product"
  type: boolean
  inputBinding:
    prefix: --sum
- id: in_include_original_genotypes
  doc: "include original genotypes in output. When multiple\nvariants are merged,\
    \ the last will dictate the\ngenotype field"
  type: boolean
  inputBinding:
    prefix: -g
- id: in_weighting_scheme_intended
  doc: "weighting scheme (intended for use in tiered merging),\noptions: unweighted,\
    \ carrier_wt, evidence_wt"
  type: string
  inputBinding:
    prefix: -w
- id: in_tempdir
  doc: Directory for temp file downloads
  type: File
  inputBinding:
    prefix: --tempdir
outputs:
- id: out_stdout
  doc: Standard output stream
  type: stdout
cwlVersion: v1.1
baseCommand:
- svtools
- lmerge
