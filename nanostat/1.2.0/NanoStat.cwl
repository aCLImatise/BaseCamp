class: CommandLineTool
id: NanoStat.cwl
inputs:
- id: outdir
  doc: Specify directory for output, only in combination with -n.
  type: string
  inputBinding:
    prefix: --outdir
- id: prefix
  doc: Specify an optional prefix to be used for the output file.
  type: string
  inputBinding:
    prefix: --prefix
- id: name
  doc: Specify a filename/path for the output, stdout is the default.
  type: string
  inputBinding:
    prefix: --name
- id: threads
  doc: Set the allowed number of threads to be used by the script.
  type: string
  inputBinding:
    prefix: --threads
- id: barcoded
  doc: Use if you want to split the summary file by barcode
  type: boolean
  inputBinding:
    prefix: --barcoded
- id: read_type
  doc: '{1D,2D,1D2} Which read type to extract information about from summary. Options
    are 1D, 2D, 1D2'
  type: boolean
  inputBinding:
    prefix: --readtype
- id: fast_q
  doc: Data is in one or more (compressed) fastq file(s).
  type: File[]
  inputBinding:
    prefix: --fastq
- id: fast_a
  doc: Data is in one or more (compressed) fasta file(s).
  type: File[]
  inputBinding:
    prefix: --fasta
- id: summary
  doc: Data is in one or more (compressed) summary file(s)generated by albacore or
    guppy.
  type: File[]
  inputBinding:
    prefix: --summary
- id: bam
  doc: Data is in one or more sorted bam file(s).
  type: File[]
  inputBinding:
    prefix: --bam
- id: ub_am
  doc: Data is in one or more unmapped bam file(s).
  type: File[]
  inputBinding:
    prefix: --ubam
- id: cram
  doc: Data is in one or more sorted cram file(s).
  type: File[]
  inputBinding:
    prefix: --cram
outputs: []
cwlVersion: v1.1
baseCommand:
- NanoStat
