class: CommandLineTool
id: checkTableCoords.cwl
inputs:
- id: database
  doc: ''
  type: string
  inputBinding:
    position: 0
- id: tablename
  doc: ''
  type: string
  inputBinding:
    position: 1
- id: table
  doc: 'Check this table only.  (Default: all tables)'
  type: string
  inputBinding:
    prefix: -table
- id: days_old
  doc: Check tables that have been modified at most N days ago.
  type: string
  inputBinding:
    prefix: -daysOld
- id: hours_old
  doc: Check tables that have been modified at most N hours ago. (days and hours are
    additive)
  type: string
  inputBinding:
    prefix: -hoursOld
- id: exclude
  doc: Exclude tables matching any pattern in comma-separated patList.  patList can
    contain wildcards (*?) but should be escaped or single-quoted if it does.  patList
    can contain "genbank" which will be expanded to all tables generated by the automated
    genbank build process.
  type: string
  inputBinding:
    prefix: -exclude
- id: ignore_blocks
  doc: To save time (but lose coverage), skip block coord checks.
  type: boolean
  inputBinding:
    prefix: -ignoreBlocks
- id: verbose_blocks
  doc: Print out more details about illegal block coords, since  they can't be found
    by simple SQL queries.
  type: boolean
  inputBinding:
    prefix: -verboseBlocks
outputs: []
cwlVersion: v1.1
baseCommand:
- checkTableCoords
