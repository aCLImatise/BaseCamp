class: CommandLineTool
id: phyluce_align_add_missing_data_designators.cwl
inputs:
- id: alignments
  doc: 'Alignment files to process (default: None)'
  type: string
  inputBinding:
    prefix: --alignments
- id: output
  doc: 'The output dir in which to store copies of the alignments (default: None)'
  type: string
  inputBinding:
    prefix: --output
- id: match_count_output
  doc: 'The output file containing taxa and loci in complete/incomplete matrices generated
    by get_match_counts.py. (default: None)'
  type: string
  inputBinding:
    prefix: --match-count-output
- id: incomplete_matrix
  doc: 'The output file for incomplete-matrix records generated by get_match_counts.py.
    (default: None)'
  type: string
  inputBinding:
    prefix: --incomplete-matrix
- id: min_tax_a
  doc: 'The minimum number of taxa to keep (default: 3)'
  type: long
  inputBinding:
    prefix: --min-taxa
- id: missing_character
  doc: 'The character to use for missing data (default: ?)'
  type: string
  inputBinding:
    prefix: --missing-character
- id: verbatim
  doc: 'Do not parse species names at all - use them verbatim (default: False)'
  type: boolean
  inputBinding:
    prefix: --verbatim
- id: input_format
  doc: 'The input alignment format. (default: nexus)'
  type: string
  inputBinding:
    prefix: --input-format
- id: output_format
  doc: 'The output alignment format. (default: nexus)'
  type: string
  inputBinding:
    prefix: --output-format
- id: no_check_missing
  doc: 'Do not check the list of missing loci (default: True)'
  type: boolean
  inputBinding:
    prefix: --no-check-missing
- id: verbosity
  doc: 'The logging level to use. (default: INFO)'
  type: string
  inputBinding:
    prefix: --verbosity
- id: log_path
  doc: 'The path to a directory to hold logs. (default: None)'
  type: string
  inputBinding:
    prefix: --log-path
- id: cores
  doc: 'Process alignments in parallel using --cores for alignment. This is the number
    of PHYSICAL CPUs. (default: 1)'
  type: string
  inputBinding:
    prefix: --cores
outputs: []
cwlVersion: v1.1
baseCommand:
- phyluce_align_add_missing_data_designators
