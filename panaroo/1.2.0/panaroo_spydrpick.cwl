class: CommandLineTool
id: panaroo_spydrpick.cwl
inputs:
- id: quantile
  doc: the quantile used to determine a threshold for keeping MI values (default=0.9).
  type: string
  inputBinding:
    prefix: --quantile
- id: input
  doc: gene presence absence file generated by Panaroo
  type: string
  inputBinding:
    prefix: --input
- id: out_dir
  doc: location of an output directory
  type: string
  inputBinding:
    prefix: --out_dir
- id: tree
  doc: phylogeny in newick format for weigting samples to control for population structure
  type: string
  inputBinding:
    prefix: --tree
- id: clusters
  doc: "sample clusters for weigting to control for population structure. format:\
    \ 'sample_name,cluster_id'"
  type: string
  inputBinding:
    prefix: --clusters
outputs: []
cwlVersion: v1.1
baseCommand:
- panaroo-spydrpick
