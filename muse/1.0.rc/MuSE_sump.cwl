class: CommandLineTool
id: ../../../MuSE_sump.cwl
inputs:
- id: single_input_file
  doc: single input file generated by 'MuSE call'
  type: File
  inputBinding:
    prefix: -I
- id: input_generated_whole_genome
  doc: input generated from whole genome sequencing data
  type: boolean
  inputBinding:
    prefix: -G
- id: input_generated_whole_exome
  doc: input generated from whole exome sequencing data
  type: boolean
  inputBinding:
    prefix: -E
- id: output_file_name
  doc: output file name (VCF format)
  type: string
  inputBinding:
    prefix: -O
- id: dbsnp_vcf_file
  doc: dbSNP vcf file that should be bgzip compressed, tabix indexed and based on
    the same reference genome used in 'MuSE call'
  type: File
  inputBinding:
    prefix: -D
outputs: []
cwlVersion: v1.1
baseCommand:
- MuSE
- sump
