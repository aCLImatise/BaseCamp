class: CommandLineTool
id: MuSE_sump.cwl
inputs:
- id: i
  doc: single input file generated by 'MuSE call'
  type: File
  inputBinding:
    prefix: -I
- id: g
  doc: input generated from whole genome sequencing data
  type: boolean
  inputBinding:
    prefix: -G
- id: e
  doc: input generated from whole exome sequencing data
  type: boolean
  inputBinding:
    prefix: -E
- id: o
  doc: output file name (VCF format)
  type: string
  inputBinding:
    prefix: -O
- id: d
  doc: dbSNP vcf file that should be bgzip compressed, tabix indexed and based on
    the same reference genome used in 'MuSE call'
  type: File
  inputBinding:
    prefix: -D
outputs: []
cwlVersion: v1.1
baseCommand:
- MuSE
- sump
