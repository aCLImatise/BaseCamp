class: CommandLineTool
id: ../../../metaplots.cwl
inputs:
- id: an_not_dir
  doc: transcripts annotation directory, generated by prepare_transcripts.
  type: string
  inputBinding:
    prefix: --annot_dir
- id: rpf_mapping_file
  doc: ribo-seq BAM/SAM file aligned to the transcriptome.
  type: string
  inputBinding:
    prefix: --rpf_mapping_file
- id: input_file
  doc: the file list the ribo-seq BAM/SAM files aligned to the transcriptome.
  type: string
  inputBinding:
    prefix: --input_file
- id: stranded
  doc: 'whether the data is strand-specific, reverse means reversed strand interpretation.(default:
    yes)'
  type: string
  inputBinding:
    prefix: --stranded
- id: minimum_length
  doc: minimum length of read to output, default 24
  type: long
  inputBinding:
    prefix: --minimum-length
- id: maximum_length
  doc: maximum length of read to output, default 36
  type: long
  inputBinding:
    prefix: --maximum-length
- id: p_value_one_cut_off
  doc: pvalue cutoff of frame0 > frame2 for automatically predicting P-site location,
    default 0.001
  type: string
  inputBinding:
    prefix: --pvalue1_cutoff
- id: p_value_two_cut_off
  doc: pvalue cutoff of frame0 > frame2 for automatically predicting P-site location,
    default 0.001
  type: string
  inputBinding:
    prefix: --pvalue2_cutoff
- id: frame_zero_percent
  doc: proportion threshold of the number of reads in frame0, defined by f0/(f0+f1+f2),
    default 0.65
  type: string
  inputBinding:
    prefix: --frame0_percent
- id: out_name
  doc: 'name of output pdf file(default: metaplots)'
  type: string
  inputBinding:
    prefix: --outname
outputs: []
cwlVersion: v1.1
baseCommand:
- metaplots
