#!/usr/bin/env cwl-runner

baseCommand:
- anvi-script-get-hmm-hits-per-gene-call
class: CommandLineTool
cwlVersion: v1.0
id: anvi-script-get-hmm-hits-per-gene-call
inputs:
- doc: Anvi'o contigs database generated by 'anvi-gen- contigs'
  id: contigs_db
  inputBinding:
    prefix: --contigs-db
  type: string
- doc: NAME Use a specific HMM source. You can use '--list-hmm- sources' flag to see
    a list of available resources. The default is 'None'.
  id: hmm_source
  inputBinding:
    prefix: --hmm-source
  type: string
- doc: File path to store results.
  id: output_file
  inputBinding:
    prefix: --output-file
  type: File
