version 1.0

task AnviScriptCheckmTreeToInteractive {
  input {
    String? tree__tree
    Directory? output_dir
    String tree
  }
  command <<<
    anvi-script-checkm-tree-to-interactive \
      ~{tree} \
      ~{if defined(tree__tree) then ("-t " +  '"' + tree__tree + '"') else ""} \
      ~{if defined(output_dir) then ("--output-dir " +  '"' + output_dir + '"') else ""}
  >>>
  parameter_meta {
    tree__tree: "TREE, --tree CHECKM TREE Tree file generated by CheckM."
    output_dir: "The directory name that output files will be stored."
    tree: ""
  }
}