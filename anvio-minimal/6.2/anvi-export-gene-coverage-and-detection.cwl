#!/usr/bin/env cwl-runner

baseCommand:
- anvi-export-gene-coverage-and-detection
class: CommandLineTool
cwlVersion: v1.0
id: anvi-export-gene-coverage-and-detection
inputs:
- doc: Anvi'o profile database
  id: profile_db
  inputBinding:
    prefix: --profile-db
  type: string
- doc: Anvi'o contigs database generated by 'anvi-gen- contigs'
  id: contigs_db
  inputBinding:
    prefix: --contigs-db
  type: string
- doc: A prefix to be used while naming the output files (no file type extensions
    please; just a prefix).
  id: output_file_prefix
  inputBinding:
    prefix: --output-file-prefix
  type: File
