class: CommandLineTool
id: ../../../convert_sam_for_rsem.cwl
inputs:
- id: p_slash_num_threads
  doc: 'Set the number of threads to be used for converting. (Default: 1)'
  type: long
  inputBinding:
    prefix: -p/--num-threads
- id: memory_per_thread
  doc: "Set the maximum allowable memory per thread. <string> represents the memory\
    \ and accepts suffices 'K/M/G'. (Default: 1G)"
  type: string
  inputBinding:
    prefix: --memory-per-thread
- id: h_slash_help
  doc: Show help information.
  type: boolean
  inputBinding:
    prefix: -h/--help
- id: input_dots_am_slash_input_dot_bam_slash_input_dot_cram
  doc: The SAM/BAM/CRAM file generated by user's aligner. We require this file contains
    the header section.
  type: string
  inputBinding:
    position: 0
- id: output_file_name
  doc: The output name for the converted file. 'convert-sam-for-rsem' will output
    a BAM with the name 'output_file_name.bam'.
  type: string
  inputBinding:
    position: 1
outputs: []
cwlVersion: v1.1
baseCommand:
- convert-sam-for-rsem
